#include <stdio.h>
 
int main() {
 
    float i, j = 1, g;
    int n;

    for(i = 0; i < 2.2; i += 0.2){
	    g = j;
        for(n = 3; n > 0; n--){
            if(i == 0.0 || i == 1.0 || i > 1.9)
                printf("I=%.0f J=%.0f\n", i, j);
            else
                printf("I=%.1f J=%.1f\n", i, j);
	        j++;
	    }
	    j = g;
        j += 0.2;
    }
 
    return 0;
}
